{
  "name": "eventpanel",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "---BACKEND---": "----------------------------------------------------------------------------------------------",
    "back-api:dev": "cross-env STAGE=dev nx serve back-api --host 0.0.0.0",
    "---FRONTENT---": "---------------------------------------------------------------------------------------------",
    "front-client:dev": "nx serve front-client --host 0.0.0.0",
    "front-ui:storybook": "nx reset & nx run front-ui:storybook",
    "---APP---": "---------------------------------------------------------------------------------------------",
    "app:dev": "concurrently \"yarn run back-api:dev\" \"yarn run front-client:dev\"",
    "---CODEBASE---": "---------------------------------------------------------------------------------------------",
    "lint": "nx format:write && nx run-many --all --target=lint --fix",
    "affected:lint": "nx format:write && nx affected:lint --fix",
    "type-check": "nx run-many --all --target=type-check",
    "affected:type-check": "nx affected --target=type-check",
    "test": "cross-env STAGE=test nx run-many --all --target=test",
    "affected:test": "cross-env STAGE=test nx affected:test",
    "e2e": "cross-env STAGE=test nx run-many --all --target=e2e",
    "affected:e2e": "cross-env STAGE=test nx affected:e2e"
  },
  "private": true,
